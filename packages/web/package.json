{
  "name": "@ai-terminal-ts/web",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "scripts": {
    "dev": "bun run dev:build:client & bun run start",
    "start": "bun run --watch src/server.ts",
    "build": "bun run src/build-client.js",
    "dev:build:css": "tailwindcss -i ./src/styles/styles.css -o ./static/build.css --watch",
    "dev:build:client": "bun run src/build-client.js --watch",
    "build:client": "bun src/build-client.js",
    "build:client:min": "BUILD_MINIFY=true node src/build-client.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/zod-validator": "^0.7.4",
    "@tailwindcss/cli": "^4.1.16",
    "bun": "^1.3.1",
    "hono": "^4.10.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7"
  }
}
